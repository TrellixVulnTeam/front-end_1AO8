{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "@crowsong/image": {
      "projectType": "library",
      "root": "projects/crowsong/image",
      "sourceRoot": "projects/crowsong/image/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "projects/crowsong/image/tsconfig.lib.json",
            "project": "projects/crowsong/image/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/crowsong/image/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/crowsong/image/src/test.ts",
            "tsConfig": "projects/crowsong/image/tsconfig.spec.json",
            "karmaConfig": "projects/crowsong/image/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/crowsong/image/tsconfig.lib.json",
              "projects/crowsong/image/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }, 
    "@crowsong/list": {
      "projectType": "library",
      "root": "projects/crowsong/list",
