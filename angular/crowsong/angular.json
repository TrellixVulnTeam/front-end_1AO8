              "projects/crowsong/title/tsconfig.lib.json",
              "projects/crowsong/title/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "crowsong-messaging": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "projects/crowsong-messaging",
      "sourceRoot": "projects/crowsong-messaging/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/crowsong-messaging",
            "index": "projects/crowsong-messaging/src/index.html",
            "main": "projects/crowsong-messaging/src/main.ts",
            "polyfills": "projects/crowsong-messaging/src/polyfills.ts",
            "tsConfig": "projects/crowsong-messaging/tsconfig.app.json",
            "aot": true,
            "assets": [
              "projects/crowsong-messaging/src/favicon.ico",
              "projects/crowsong-messaging/src/assets"
            ],
            "styles": [
              "projects/crowsong-messaging/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/crowsong-messaging/src/environments/environment.ts",
                  "with": "projects/crowsong-messaging/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "crowsong-messaging:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "crowsong-messaging:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "crowsong-messaging:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/crowsong-messaging/src/test.ts",
            "polyfills": "projects/crowsong-messaging/src/polyfills.ts",
            "tsConfig": "projects/crowsong-messaging/tsconfig.spec.json",
            "karmaConfig": "projects/crowsong-messaging/karma.conf.js",
            "assets": [
              "projects/crowsong-messaging/src/favicon.ico",
              "projects/crowsong-messaging/src/assets"
            ],
            "styles": [
              "projects/crowsong-messaging/src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/crowsong-messaging/tsconfig.app.json",
              "projects/crowsong-messaging/tsconfig.spec.json",
              "projects/crowsong-messaging/e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/crowsong-messaging/e2e/protractor.conf.js",
            "devServerTarget": "crowsong-messaging:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "crowsong-messaging:serve:production"
            }
          }
        }
      }
    },
    "crowsong-software": {
      "projectType": "application",
