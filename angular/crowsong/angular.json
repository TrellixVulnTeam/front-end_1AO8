    "@crowsong/title": {
      "projectType": "library",
      "root": "projects/crowsong/title",
      "sourceRoot": "projects/crowsong/title/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "projects/crowsong/title/tsconfig.lib.json",
            "project": "projects/crowsong/title/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/crowsong/title/tsconfig.lib.prod.json"
            }
